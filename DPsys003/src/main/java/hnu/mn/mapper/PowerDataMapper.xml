<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hnu.mn.mapper.PowerDataMapper">


	<!--根据数据权限ID查找数据权限 -->
	<select id="selByPowerDataID"
		resultType="hnu.mn.pojo.PowerData">
		select * from t_powerData where
		powerDataID=#{powerDataID};
	</select>



</mapper>