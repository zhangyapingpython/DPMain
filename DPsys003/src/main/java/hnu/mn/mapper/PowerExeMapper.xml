<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hnu.mn.mapper.PowerExeMapper">


	<!--根据执行权限ID查找权限权限 -->
	<select id="selByPowerExeID"
		resultType="hnu.mn.pojo.PowerExe">
		select * from t_powerExe where
		powerExeID=#{powerExeID};
	</select>



</mapper>